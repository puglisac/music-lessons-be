{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [{
        "handle": "apple",
        "name": "apple computers",
        "num_employees": 598,
        "description": "we make computers",
        "logo_url": "https://image.shutterstock.com/image-vector/internet-design-logoicon-template-260nw-1037418430.jpg"
    }],
    "required": [
        "handle",
        "name",
        "num_employees",
        "description",
        "logo_url"
    ],
    "properties": {
        "handle": {
            "$id": "#/properties/handle",
            "type": "string",
            "title": "The handle schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "apple"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "apple computers"
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                598
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "we make computers"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://image.shutterstock.com/image-vector/internet-design-logoicon-template-260nw-1037418430.jpg"
            ]
        }
    },
    "additionalProperties": true
}