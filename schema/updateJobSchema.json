    "default": {},
    "examples": [{
        "title": "job title",
        "salary": 20000.5,
        "equity": 0.5,
        "company_handle": "apple"
    }],
    "required": [

    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
